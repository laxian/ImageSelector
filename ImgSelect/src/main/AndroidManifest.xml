    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.haozhn.imageselector.AlbumActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.haozhn.imageselector.ImageActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.haozhn.imageselector.PreviewActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.haozhn.imageselector.BigImageActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>